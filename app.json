{
  "expo": {
    "name": "SOS Vida",
    "slug": "sosvida",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo_transparente.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "resizeMode:": "cover",
      "justifyContent": "center",
      "width": 500,
      "height": 500,
      "flexDirection": "column",
      "alignItems": "center"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_transparente.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.delsantx.sosvida"
    },
    "web": {
      "favicon": "./assets/logo_transparente.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/RubikMicrobe-Regular.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "80f4f7c6-ac33-4130-8338-2499cffff636"
      }
    }
  }
}
